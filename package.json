{
  "name": "floema-study",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "frontend:build": "webpack --progress --config webpack.config.build.js",
    "frontend:development": "webpack serve --progress --config webpack.config.development.js",
    "backend:build": "node app.js",
    "backend:development": "nodemon app.js",
    "start:frontend": "npm run frontend:development",
    "start:backend": "npm run backend:development",
    "start": "concurrently --kill-others \"npm run backend:development\" \"npm run frontend:development\""
  },
  "author": "Cassiano Kruchelski Vidal",
  "devDependencies": {
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "image-minimizer-webpack-plugin": "^4.1.3",
    "imagemin": "^9.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^8.0.0",
    "imagemin-optipng": "^8.0.0",
    "include-media": "^1.4.10",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^2.0.15",
    "postcss-loader": "^8.1.1",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "terser-webpack-plugin": "^5.3.11",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@prismicio/client": "^6.4.3",
    "@prismicio/helpers": "^2.3.0",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.2.4",
    "pug": "^3.0.2"
  }
}
